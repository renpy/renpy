    # game/tutorial_atl.rpy:270
    old "xpos 1.0 ypos .5"
    new "xpos 1.0 ypos .5"

    # game/tutorial_atl.rpy:270
    old "xpos .75 ypos .25"
    new "xpos .75 ypos .25"

