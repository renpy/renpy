init python:

    layout.provides('load_save')

label load_screen:

    $ renpy.show_screen('load')
    $ ui.interact()
    jump _noisy_return

label save_screen:

    $ renpy.show_screen('save')
    $ ui.interact()
    jump _noisy_return

