    # game/script.rpy:2
    old "Sylvie"
    new "Sylvie"

    # game/script.rpy:3
    old "Me"
    new "Mig"

    # game/script.rpy:40
    old "As soon as she catches my eye, I decide..."
    new "Så snart hun fanger mit øje, beslutter jeg mig for..."

